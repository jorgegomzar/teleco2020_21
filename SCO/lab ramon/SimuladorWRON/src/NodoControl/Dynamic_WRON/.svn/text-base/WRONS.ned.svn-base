package Control.WRON;

//
// TODO auto-generated module
//
module WRONS
{
        parameters:
        double num_nodos;
        double num_edge;
        double num_txrx_max;
        double num_lng_ond_max;
        double tasa @unit(bps);
        double porcen_trafico;
        double multi_trafico;
        double traf_alto_bajo;
        double porcentaje_cambio;
        string algoritmo;
        double num_tx;
        double tiempo_corto @unit(s);
        double tiempo_largo @unit(s);
        double gran_tiempo @unit(s);


        @display("bgb=216,191,#aecaee,#5aa206,2;bgp=18,10");

        submodules:

        fuente_trafico: Fuente_trafico { //
            parameters:
                num_edge = num_edge;
                tasa = tasa;
                porcen_trafico = porcen_trafico;
                multi_trafico = multi_trafico;
                traf_alto_bajo = traf_alto_bajo;
                porcentaje_cambio = porcentaje_cambio;

                @display("i=block/source;p=65,100;b=32,30");

        }
        control_wrons: Control_WRONS {
            parameters:
                num_nodos = num_nodos;
                num_edge = num_edge;
                num_txrx_max = num_txrx_max;
                num_lng_ond_max = num_lng_ond_max;
                algoritmo = algoritmo;
                num_tx = num_tx;
                tiempo_corto = tiempo_corto;
                tiempo_largo = tiempo_largo;
                gran_tiempo = gran_tiempo;


                @display("p=183,100;i=block/star;b=32,30");

        }
    connections allowunconnected:

        fuente_trafico.out --> control_wrons.in;
    
}
