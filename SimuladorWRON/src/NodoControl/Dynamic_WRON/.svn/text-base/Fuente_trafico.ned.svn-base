package Control.WRON;

//
// TODO auto-generated module
//
module Fuente_trafico
{
        parameters:
        double num_edge;
        double int_cambio @unit(s);
        double tasa @unit(bps);
        double porcen_trafico;
        double multi_trafico;
        double traf_alto_bajo;
        double porcentaje_cambio;




        @display("bgb=400,367,#aecaee,#5aa206,2;bgp=10,10");
    gates:
        output out;



    submodules:

        cambio_matriz_trafico: Generar_matriz { //
            parameters:
                int_cambio = int_cambio; // Intervalo entre llegadas


                @display("i=gen;p=90,172;b=32,30");

        }
        crea_matriz: Crea_matriz { //
            parameters:
                num_edge = num_edge;
                tasa = tasa;
                porcen_trafico = porcen_trafico;
                multi_trafico = multi_trafico;
                traf_alto_bajo = traf_alto_bajo;
                porcentaje_cambio = porcentaje_cambio;


                @display("p=250,172;b=100,30,,#7acc2a,#7b9629,3");

        }
    connections allowunconnected:

        cambio_matriz_trafico.out --> crea_matriz.in;
        crea_matriz.out --> out;

    
    
}
